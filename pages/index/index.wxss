
.content{
  padding: 20rpx;
  box-sizing: border-box
}
.position{
  display: flex;
  justify-content: space-between;
  line-height: 40rpx;
  font-size: 26rpx;
}
.position .place{
  min-width:100rpx;
  display: flex;
  text-align: center;
  justify-content: center;
  align-content: center;
  margin: auto
}
.position .place .positionImg{
  height: 40rpx;
  padding-top: 6rpx;
  box-sizing: border-box
}
.position .place image{
  height: 24rpx;
  width: 24rpx;
}
.search{
  height: 50rpx;
  flex: 1;
  line-height: 50rpx;
  padding: 0 10rpx;
}
.search .searchIpt{
  height: 50rpx;
  background-color: rgb(243, 243, 243);
  min-height: 50rpx;
  border-radius: 25rpx;
  text-indent: 30rpx;
  text-align: center;
}
.search .placeholderClass{
  text-align: center;
  line-height: 40rpx;
}

.classIconList,.sIconList{
  display: flex;
  justify-content: space-around;
  margin-top: 40rpx;
  box-sizing: border-box;
  flex-wrap: wrap
}
.classIconListItem,.sIconListItem{
  width: 18%;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 20rpx 0 0;
  flex-wrap: wrap
}
.itemBack{
  width: 70rpx;
  height: 70rpx;
  border-radius: 35rpx;
  background: red;
  text-align: center;
  line-height: 70rpx
}
.itemBack .iconfont{
  color: white;
  font-size: 34rpx;
}
.classTitel,.sclassTitel{
  display: block;
  width: 100%;
  text-align: center;
  line-height: 50rpx;
  font-size: 24rpx;
  color: rgb(61, 61, 61);
}
.sclassTitel{
  font-size: 20rpx;
}
.sIconList .sIconListItem .iconfont{
  font-size: 38rpx;
  color: blue
}
.sIconList{
  margin-top: 0
}
.fulicenter{
  height: 120rpx;
  width: 100%;
  margin: 30rpx 0;
  border-radius: 10rpx;
  border: 1rpx solid rgb(216, 216, 216);
  box-shadow: 0rpx 1rpx 1rpx rgb(209, 209, 209);
  display: flex;
  justify-content: space-around
}
.fulicenter .quan{
  width:150rpx;
  height: 100%;
  position: relative
}
.fulicenter .quanText{
  flex: 1;
}
.ttlq{
  position: absolute;
  top: 15rpx;
  left: 15rpx;
  width: 85rpx;
  height: 30rpx;
  background: orangered;
  border-radius: 14rpx 0;
  text-align: center;
  line-height: 30rpx;
  color: white;
  font-size: 15rpx;
}
.quanImg{
  height: 80rpx;
  width: 80rpx;
  position: absolute;
  top: 50%;
  margin-top: -40rpx;
  margin-left: 40rpx;
  z-index: -1
}
.quanImg image{
  height: 80rpx;
  width: 80rpx;
}
.flTitle{
  line-height: 55rpx;
  font-size:30rpx;
  box-sizing: border-box
}
.flSpan{
  display: inline-block;
  font-size: 20rpx;
  color: rgb(180, 180, 180)
}
.flCircleLine{
  display: flex;
  align-content: center;
}
.flCircleItem{
  width: 56rpx;
  height: 20rpx;
  display: flex;
  justify-content: space-around;
  align-content: center;
}
.flCircle{
  width: 34rpx;
  height: 34rpx;
  border-radius: 17rpx;
  background: rgb(218, 218, 218);
  display: inline-block;
  font-size: 14rpx;
  text-align: center;
  line-height: 34rpx;
}
.flLine{
  display: inline-block;
  width: 17rpx;
  height: 4rpx;
  background: rgb(185, 185, 185);
  margin: 15rpx 0;
  border-radius: 2rpx;
}
.lastLine{
  background: white
}
.ptTitle{
  font-size: 28rpx;
  color: rgb(134, 134, 134);
  margin-bottom: 30rpx;
}
