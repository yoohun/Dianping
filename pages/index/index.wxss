.ptList{
  padding: 20rpx 20rpx;
  box-sizing: border-box;
  width: 90%;
  border-radius: 10rpx;
  display: inline-block;
  border: 1rpx solid rgb(216, 216, 216);
  box-shadow: 0 5rpx 3rpx rgb(236, 236, 236);
  margin-right: 5%;
  /* float: left; */
}
.ptItemTitle{
  width: 100%;
  display: flex;
  justify-content: space-around;
  border-bottom: 1rpx solid rgb(216, 216, 216);
  line-height: 40rpx;
}
.ptItemIcon{
flex: 1;
padding: 0;
height: 40rpx;
line-height: 40rpx;
}
.ptItemDistance{
  padding: 0 10rpx;
  font-size: 22rpx;
}
.ptItemDistance .iconfont{
  display: inline-block;
  font-size:16rpx;
}
.ptItemIconTitle{
  margin-left: 10rpx;
  font-size: 18rpx;
  display: inline-block;
}
.ptItemIcon .iconfont{
  display: inline-block;
}
.ptItemContent{
  padding: 10rpx 0;
  display: flex;
  justify-content: space-between
}
.ptItemContentImg{
  width: 130rpx;
  height: 130rpx;
  border: 1rpx solid aliceblue;
}
.ptItemContentImg image{
  height: 130rpx;
  width: 130rpx;
}
.ptItemContentText{
  flex: 1;
  padding-left: 20rpx;
}
.ptItemContentTextT{
  font-size: 28rpx;
  font-weight: bold;
  line-height: 40rpx
}
.ptItemContentTextP{
  height: 55rpx;
  display: flex;
  box-sizing: border-box
}
.ptItemContentTextPK{
  width: 100rpx;
  height: 30rpx;
  border: 1rpx solid orangered;
  font-size: 18rpx;
  text-align: center;
  line-height: 24rpx;
  color: orangered;
  border-radius: 4rpx;
  display: inline-block;
  margin: 12rpx 0 0
}
.ptItemContentTextPPrice{
  font-size: 28rpx;
  color: orangered;
  height: 40rpx;
  display: inline-block;
  margin: 12rpx 10rpx 0;
  line-height: 40rpx;
}
.ptItemContentTextO{
  font-size: 22rpx;
  display: flex;
  color: gray;
  justify-content: space-between
}
.like{
  margin-top: 60rpx; 
}
.likeItem{
  padding: 20rpx 20rpx 30rpx;
  border-bottom: 1rpx solid gainsboro;
  display: flex;
  justify-content: space-between
}
.likeItemImg{
  height: 160rpx;
  width: 160rpx;
  margin-right: 20rpx;
}
.likeItemImg image{
  height: 160rpx;
  width: 160rpx;
}
.likeItem .likeItemTitle{
  display: flex;
  justify-content: space-between

}
.likeItem .ptItem{
  flex: 1;
  display: flex;
  justify-content: space-between;
  flex-direction: column
}
.likeItemTitle view:last-child{
  font-size: 18rpx;
}
.likeItemB{
  display: flex;
  justify-content: space-between;
  color: gray;
  font-size: 20rpx;
}
.likeItemB view{
  display: inline-block
}
.likeItemBP{
  text-decoration: line-through;
  color: gray;
}
.likeItemB .likeItemBPOrange{
  color: orangered;
  text-decoration: none;
  font-size: 24rpx;
  margin-right: 20rpx;
}
.likeItemM{
  font-size: 20rpx;
}


::-webkit-scrollbar{
    width: 0;
    height: 0;
    color: transparent;
}
